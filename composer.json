{
    "name": "raxos/router",
    "description": "A fast and simple router, built with php attributes.",
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "authors": [
        {
            "name": "Bas Milius",
            "email": "bas@mili.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raxos\\Router\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
    "require": {
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "jetbrains/phpstorm-attributes": "^1.2",
        "raxos/foundation": "*",
        "raxos/http": "*"
    },
    "require-dev": {
        "raxos/database": "*"
    },
    "suggest": {
        "raxos/database": "to use the MapModel router parameter attribute."
    }
}
