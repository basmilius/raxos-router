{
	"name": "raxos/router",
	"description": "A fast and simple router, built with php attributes.",
	"license": "mit",
    "minimum-stability": "dev",
    "prefer-stable" : true,
	"type": "library",
	"authors": [
		{
			"name": "Bas Milius",
			"email": "bas@mili.us"
		}
	],
	"autoload": {
		"psr-4": {
			"Raxos\\Router\\": "src/Raxos/Router"
		}
	},
	"config": {
		"platform": {
			"php": "8.3"
		}
	},
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
	"require": {
		"ext-json": "*",
		"ext-simplexml": "*",
		"jetbrains/phpstorm-attributes": "^1.1",
        "raxos/foundation": "*",
		"raxos/http": "*"
	}
}
